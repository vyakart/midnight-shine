# Environment variables for ETH donation setup
# Copy this to .env and fill in your values

# Infura API Key (for RPC calls and contract deployment)
INFURA_API_KEY=your_infura_api_key_here

# Etherscan API Key (for contract verification)
ETHERSCAN_API_KEY=your_etherscan_api_key_here

# Private key for contract deployment (NEVER commit this!)
PRIVATE_KEY=0x_your_private_key_here

# WalletConnect Project ID (for WalletConnect v2)
WALLETCONNECT_PROJECT_ID=your_walletconnect_project_id_here